{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/react"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/json-server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.html"
        }
    ]
}
